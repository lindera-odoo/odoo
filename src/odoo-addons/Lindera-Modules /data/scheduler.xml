<odoo>
    <data>
        <record id="office365_cron_mails" model="ir.cron">
            <field name="name">Mail synchronisation</field>
            <field name="model_id" ref="model_lindera_office_mail"/>
            <field name="state">code</field>
            <field name="active" eval="False"/>
            <field name="code">model.syncMailsScheduler()</field>
            <field name='interval_number'>1</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="office365_cron_mail_cleaner" model="ir.cron">
            <field name="name">Office Conversations aufräumen</field>
            <field name="model_id" ref="model_lindera_office_mail_cleaner"/>
            <field name="state">code</field>
            <field name="active" eval="False"/>
            <field name="code">model.cleanConversations()</field>
            <field name='interval_number'>30</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">0</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="invoice_cleaner" model="ir.cron">
            <field name="name">Office Conversations aufräumen</field>
            <field name="model_id" ref="model_lindera_invoice_cleaner"/>
            <field name="state">code</field>
            <field name="active" eval="False"/>
            <field name="code">model.cleanInvoices()</field>
            <field name='interval_number'>30</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">0</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="office365_cron_contacts" model="ir.cron">
            <field name="name">Contacts synchronisation</field>
            <field name="model_id" ref="model_lindera_office_contact"/>
            <field name="state">code</field>
            <field name="active" eval="False"/>
            <field name="code">model.syncContactsScheduler()</field>
            <field name='interval_number'>30</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="office365_cron_clean_contacts" model="ir.cron">
            <field name="name">Contacts Cleanup</field>
            <field name="model_id" ref="model_lindera_office_contact"/>
            <field name="state">code</field>
            <field name="active" eval="False"/>
            <field name="code">model.syncContactsCleaner()</field>
            <field name='interval_number'>30</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">0</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="backend_cron_submitted_at" model="ir.cron">
            <field name="name">subscription end date update</field>
            <field name="model_id" ref="model_lindera_backend_subscription"/>
            <field name="state">code</field>
            <field name="active" eval="False"/>
            <field name="code">model.update_subscription_end_date()</field>
            <field name='interval_number'>30</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">0</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="office365_cron_calendar" model="ir.cron">
            <field name="name">Calendar synchronisation</field>
            <field name="model_id" ref="model_lindera_office_calendar"/>
            <field name="state">code</field>
            <field name="active" eval="False"/>
            <field name="code">model.syncCalendarScheduler()</field>
            <field name='interval_number'>5</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="analysis_report" model="ir.cron">
            <field name="name">Monthly Analysis Report For Kunde/Pflegeberatung homes</field>
            <field name="model_id" ref="model_crm_lead"/>
            <field name="state">code</field>
            <field name="active" eval="True"/>
            <field name="code">model.generate()</field>
            <field name='interval_number'>1</field>
            <field name='interval_type'>months</field>
            <field name="numbercall">-1</field>
            <field name="user_id" ref="base.user_root"/>
        </record>
    </data>
</odoo>